{
  "extends": "angular",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "protractor": true,
    "node": true,
    "builtin": true
  },
  "globals": {
    "inject": false
  },
  "rules": {
    "angular/log": 0,
    "angular/no-service-method": 0,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-dangle": 0,
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eol-last": 2,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "one-var": 0,
    "operator-linebreak": [
      2,
      "after"
    ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-new": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "no-var": 2,
    "no-with": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi": [
      0,
      "never"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-infix-ops": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "strict": [0, "never"],
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "angular/module-getter": 0
  }
}
